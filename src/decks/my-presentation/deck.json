  "transition": "fade",
  "assetsBase": "./assets",
  "cameraOverlay": {
    "enabled": true,
    "position": "bottom-left",
    "width": "420px",
    "height": "240px",
    "borderRadius": "12px",
    "gradient": true,
    "gradientColors": ["#8b5cf6", "#ec4899", "#f59e0b"],
    "backgroundColor": "#000000",
