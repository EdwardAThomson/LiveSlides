    {
      "id": "content",
      "src": "slides/02-content.mdx",
      "layout": "center"
    },
    {
      "id": "demo",
      "src": "slides/03-demo.mdx",
      "layout": "center"
    },
    {
      "id": "closing",
      "src": "slides/04-closing.mdx",
      "layout": "center"
    }
